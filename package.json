   "scripts": {
      "install:packages": "yarn && cd store && yarn && cd ../admin && yarn",
      "dev": "concurrently \"nodemon -r esm index.js\" \"cd store && yarn dev\" \"cd admin && yarn start\"",
      "dev-mac": "concurrently \"nodemon -r esm index.js\" \"cd store && yarn dev\" \"cd admin && yarn start-mac\"",
      "build": "cd store && yarn build && cd ../admin && yarn build",
      "prod": "concurrently \"nodemon -r esm index.js\" \"cd store && yarn start\"",
      "dev:server": "concurrently \"nodemon -r esm index.js\" \"cd store && yarn dev\"",
      "download-graphql": "chmod +x ./fetchgraphqlschema.sh && ./fetchgraphqlschema.sh",
      "lint:graphql": "eslint **/graphql/*.js > graphqlerrors.txt",
      "start-hasura": "cd hasura && hasura console"
   },
